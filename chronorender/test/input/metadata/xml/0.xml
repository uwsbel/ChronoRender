<?xml version="1.0"?>
<!DOCTYPE ChronoRender>
<ChronoRender> 
    <rendersettings>
        <out>./output/xml_0/out_####.tif</out>
        <searchpaths>./:./input:./input/shaders</searchpaths>
        <framerange>0 0</framerange>
    </rendersettings>
    <shader>
        <name>plastic.sl</name>
        <Kd>666</Kd>
    </shader>
    <geometry>
        <type>sphere</type>
        <radius>0.666</radius>
        <dims>0.5 7.7</dims>
    </geometry>
    <renderobject>
        <name>tank</name>
        <range>0 100</range>
    </renderobject>
    <renderpass>
        <name>gorb</name>
        <scene>default_scene.rib</scene>
        <lighting>default_lighting</lighting>
        <resolution>640 480</resolution>
   </renderpass>
   <datasource>
       <type>csv</type>
   </datasource>
</ChronoRender>
