<?xml version="1.0"?>
<!DOCTYPE ChronoRender>
<ChronoRender> 
    <rendersettings>
        <out>./output/xml_0/out_####.tif</out>
        <in>./input/stationary/*.dat</in>
        <dataformat>ID,POS_X,POS_Y,POS_Z,EULER_X,EULER_Y,EULER_Z,ignore,ignore,ignore,ignore</dataformat>
        <delim>,</delim>
        <scaling>1.0</scaling>
        <radians>False</radians>
        <searchpaths>./:./input:./input/shaders</searchpaths>
        <framerange>0 0</framerange>
    </rendersettings>
    <shader>
        <name>plastic.sl</name>
        <Kd>666</Kd>
    </shader>
    <geometry>
        <type>sphere</type>
        <radius>0.666</radius>
        <dims>0.5 7.7</dims>
    </geometry>
    <renderobject>
        <name>tank</name>
        <range>0 100</range>
    </renderobject>
    <renderpass>
        <name>gorb</name>
        <scene>default_scene.rib</scene>
        <lighting>default_lighting</lighting>
        <resolution>640 480</resolution>
   </renderpass>
</ChronoRender>
